<UserControl x:Class="PrintMate.Terminal.Views.PrintPageView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrintMate.Terminal.Views"
             xmlns:b="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:componentsViews="clr-namespace:PrintMate.Terminal.Views.ComponentsViews"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             mc:Ignorable="d" 
             Background="Black"
             d:DesignHeight="1066" d:DesignWidth="1406">
    <Grid>

        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>
        <StackPanel>
            <Border Height="975" x:Name="LayerCanvasBorder" Visibility="Visible">
                <local:LayerCanvas x:Name="LayerCanvas"/>
            </Border>
            <Border Height="950" Visibility="Collapsed" x:Name="CameraBorder">
                <Grid>
                    <Image Stretch="Fill" x:Name="CameraImage"/>
                    <Border x:Name="CameraLoadingOverlay" Background="#80000000" Visibility="Collapsed">
                        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                            <hc:LoadingCircle Width="60" Height="60" Foreground="#ff6f00"/>
                            <TextBlock Text="Загрузка камеры..." Foreground="White" FontSize="16" FontWeight="Bold" Margin="0,15,0,0" HorizontalAlignment="Center"/>
                        </StackPanel>
                    </Border>
                </Grid>
            </Border>
            <Canvas Width="1000">
                <Image Canvas.Bottom="0" Source="/images/gradient.png" Width="1000" IsHitTestVisible="False"/>
                <Image Canvas.Bottom="0" Source="/images/gradient.png" Width="1000" IsHitTestVisible="False"/>
                <StackPanel Canvas.Bottom="20" Canvas.Left="0" Width="1000">


                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>

                        <Border VerticalAlignment="Bottom" HorizontalAlignment="Right">
                            <StackPanel>
                                <TextBlock Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="30" FontWeight="Bold">
                                    <Run Text="0" Foreground="White"/>
                                    <Run Text="ч " Foreground="Gray" FontSize="20"/>
                                    <Run Text="0" Foreground="White"/>
                                    <Run Text="м " Foreground="Gray" FontSize="20"/>
                                    <Run Text="0" Foreground="White"/>
                                    <Run Text="с" Foreground="Gray" FontSize="20"/>
                                </TextBlock>
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="Gray" FontSize="15" Margin="0 0 0 0" FontWeight="Bold">Осталось времени</TextBlock>
                            </StackPanel>
                        </Border>

                        <Border Grid.Column="1">
                            <StackPanel>
                                <Grid  HorizontalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock VerticalAlignment="Center" Foreground="White" FontSize="60" FontWeight="Bold" Text="{Binding CurrentPrintingLayer}"/>
                                    <TextBlock Grid.Column="1" VerticalAlignment="Center" Foreground="White" FontSize="30" FontWeight="Bold">/</TextBlock>
                                    <TextBlock Grid.Column="2" VerticalAlignment="Center" Foreground="White" FontSize="60" FontWeight="Bold" Text="{Binding MaxLayers}"/>
                                </Grid>
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="Gray" FontSize="15" Margin="0 0 0 0" FontWeight="Bold">Текущий слой / Всего слоев</TextBlock>
                            </StackPanel>
                        </Border>

                        <Border HorizontalAlignment="Left" VerticalAlignment="Bottom" Grid.Column="2">
                            <StackPanel>
                                <WrapPanel>
                                </WrapPanel>
                                <TextBlock Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="30" FontWeight="Bold">
                                    <Run Text="{Binding TotalPrintHours, Mode=OneWay}" Foreground="White"/>
                                    <Run Text="ч " Foreground="Gray" FontSize="20"/>
                                    <Run Text="{Binding TotalPrintMinutes, Mode=OneWay}" Foreground="White"/>
                                    <Run Text="м " Foreground="Gray" FontSize="20"/>
                                    <Run Text="{Binding TotalPrintSeconds, Mode=OneWay}" Foreground="White"/>
                                    <Run Text="с" Foreground="Gray" FontSize="20"/>
                                </TextBlock>
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="Gray" FontSize="15" Margin="0 0 0 0" FontWeight="Bold">Общее время печати</TextBlock>
                            </StackPanel>
                        </Border>
                    </Grid>


                    
                </StackPanel>
            </Canvas>
            <Border Height="85" Background="#0b0b0b" CornerRadius="10">
                <Grid VerticalAlignment="Bottom">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <!--<ColumnDefinition />-->
                    </Grid.ColumnDefinitions>

                    <Border Grid.Column="0" Background="Black" Margin="5" CornerRadius="10" Padding="15">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition />
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel VerticalAlignment="Center" Orientation="Horizontal">
                                <TextBlock VerticalAlignment="Center" Foreground="White" FontSize="20" FontWeight="Bold">Прогресс печати</TextBlock>
                                <TextBlock VerticalAlignment="Center" Margin="15 0 0 0" Foreground="Gray" FontSize="15" FontWeight="Bold">здесь отображается общий прогресс печати</TextBlock>
                            </StackPanel>
                            <TextBlock Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right" Foreground="Gray" FontSize="15" FontWeight="Bold" Text="{Binding PrintProgress, StringFormat='{}{0}%'}"/>
                            <hc:CircleProgressBar HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Column="2" Value="{Binding PrintProgress}" Foreground="#ff6f00" Margin="5,0,0,0" ShowText="False" Width="40" Height="40" ArcThickness="8" Style="{StaticResource ProgressBarWarningCircle}"/>
                        </Grid>
                    </Border>
                    <!--<Border Grid.Column="1" Background="Black" Margin="5" CornerRadius="10" Padding="15">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition />
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel VerticalAlignment="Center">
                                <TextBlock VerticalAlignment="Center" Foreground="White" FontSize="20" FontWeight="Bold">Текущий слой</TextBlock>
                                <TextBlock VerticalAlignment="Center" Foreground="Gray" FontSize="10" FontWeight="Bold">здесь отображается прогресс выполнения текущего слоя</TextBlock>
                            </StackPanel>
                            <TextBlock Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right" Foreground="Gray" FontSize="15" FontWeight="Bold" Text="{Binding LayerProgress, StringFormat='{}{0}%'}"/>
                            <hc:CircleProgressBar HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Column="2" Value="{Binding LayerProgress}" Foreground="#ff6f00" Margin="5,0,0,0" ShowText="False" Width="40" Height="40" ArcThickness="8" Style="{StaticResource ProgressBarWarningCircle}"/>
                        </Grid>
                    </Border>-->
                </Grid>
            </Border>


        </StackPanel>
        <Border Grid.Column="1" Padding="15">
            <StackPanel>
                <TextBlock HorizontalAlignment="Center" Foreground="White" FontWeight="Bold" FontSize="20" Text="{Binding MaxLayers}"/>
                <Slider IsEnabled="{Binding IsSliderEnabled}" Margin="7 10 0 0 " VerticalAlignment="Center" Foreground="OrangeRed" Height="970" IsSnapToTickEnabled="True" Value="{Binding CurrentLayer}" Minimum="0" Maximum="{Binding MaxLayers}" Orientation="Vertical"/>
                <TextBlock HorizontalAlignment="Center" Foreground="White" FontWeight="Bold" FontSize="20">0</TextBlock>
            </StackPanel>
        </Border>
        <Border Grid.Column="2" Margin="0 0 15 0" x:Name="RightMenu" Width="300" Background="Transparent" CornerRadius="0" HorizontalAlignment="Left">
                <Border.Effect>
                    <DropShadowEffect Color="Black" BlurRadius="90" Direction="6"/>
                </Border.Effect>
                <StackPanel Panel.ZIndex="9999" Margin="0 15 0 0" Cursor="">

                    <componentsViews:Container Title="Автоматический процесс" TitleSize="15">
                        <Button Background="Black" BorderBrush="#232323" Visibility="{Binding StartPrintVisibility}" Command="{Binding StartPrintCommand}" IsEnabled="{Binding StartPrintButtonIsEnabled}" FontWeight="Bold" FontSize="15" Height="50" HorizontalAlignment="Stretch" Margin="5" Style="{StaticResource ButtonSuccess}">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                <Image Source="/images/start.png" Width="15" Margin="0,0,8,0" />
                                <TextBlock Foreground="White" Text="Запустить" />
                            </StackPanel>
                        </Button>
                    <Button Background="Black" BorderBrush="#232323" Visibility="{Binding ContinuePrintVisibility}" Command="{Binding ContinuePrintCommand}" FontWeight="Bold" FontSize="15" Height="50" HorizontalAlignment="Stretch" Margin="5" Style="{StaticResource ButtonSuccess}">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                <Image Source="/images/start.png" Width="15" Margin="0,0,8,0" />
                                <TextBlock Foreground="White" Text="Возобновить" />
                            </StackPanel>
                        </Button>
                    <Button Background="Black" BorderBrush="#232323" Visibility="{Binding AbortPrintVisibility}" Command="{Binding StopPrintCommand}" FontWeight="Bold" FontSize="15" Height="50" HorizontalAlignment="Stretch" Margin="5" Style="{StaticResource ButtonDanger}">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <Image Source="/images/stop.png" Width="15" Margin="0,0,8,0" />
                            <TextBlock Foreground="White" Text="Прервать" />
                        </StackPanel>
                    </Button>
                    <Button Background="Black" BorderBrush="#232323" Visibility="{Binding PausePrintVisibility}" Command="{Binding PausePrintCommand}" FontWeight="Bold" FontSize="15" Height="50" HorizontalAlignment="Stretch" Margin="5" Style="{StaticResource ButtonPrimary}">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <Image Source="/images/pause.png" Width="15" Margin="0,0,8,0" />
                            <TextBlock Foreground="White" Text="Пауза" />
                        </StackPanel>
                    </Button>
                    </componentsViews:Container>

                    <componentsViews:Container Title="Послойный прожиг" TitleSize="15" Margin="0 20 0 0" Cursor="">
                        <TextBlock HorizontalAlignment="Left" Foreground="White" FontWeight="Bold" FontSize="12" Margin="8 0 0 0">Номер слоя</TextBlock>
                        <Grid Margin="5 0 5 0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0.30*"/>
                                <ColumnDefinition />
                            <ColumnDefinition Width="0.30*"/>
                            </Grid.ColumnDefinitions>
                            <Button Grid.Column="0" Command="{Binding AddSingleLayerIndexCommand}" Background="Black" Foreground="White" Content="+" Height="50" FontSize="15"/>
                            <Button Grid.Column="1" FontWeight="Bold" FontSize="15" Height="50" HorizontalAlignment="Stretch" Foreground="White" BorderBrush="#232323" Background="Black" Command="{Binding SelectLayerForSingleMarkCommand}" Content="{Binding SelectLayerForSingleMark}" Margin="5" />
                            <Button Grid.Column="2" Command="{Binding SuddSingleLayerIndexCommand}" Background="Black" Foreground="White" Content="-" Height="50" FontSize="15"/>

                        </Grid>
                    

                    <Button BorderBrush="#232323" Background="Black" Height="50" Command="{Binding StartSingleLayerMarkCommand}" FontWeight="Bold" FontSize="15" Visibility="{Binding StartSingleLayerMarkVisibility}" IsEnabled="{Binding StartSingleLayerMarkButtonIsEnabled}" HorizontalAlignment="Stretch" Margin="5">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <Image Source="/images/start.png" Width="15" Margin="0,0,8,0" />
                            <TextBlock Foreground="White" Text="{Binding StartSingleLayerMarkButtonText}" />
                        </StackPanel>
                    </Button>
                    <Button BorderBrush="#232323" Background="Black" FontWeight="Bold" FontSize="15" Height="50" Visibility="{Binding AbortSingleLayerMarkVisibility}" HorizontalAlignment="Stretch"  Command="{Binding StopSingleLayerMarkCommand}" Margin="5" Cursor="Hand" >
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <Image Source="/images/stop.png" Width="15" Margin="0,0,8,0" />
                            <TextBlock Foreground="White" Text="Прервать" />
                        </StackPanel>
                    </Button>
                    </componentsViews:Container>

                    <componentsViews:Container Title="Ручное дозирование" TitleSize="15" Margin="0 20 0 0" Cursor="">
                    <TextBlock HorizontalAlignment="Left" Foreground="White" FontWeight="Bold" FontSize="12" Margin="8 0 0 0">Количество доз</TextBlock>
                    <Grid Margin="5 0 5 0" Cursor="">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.30*"/>
                            <ColumnDefinition />
                            <ColumnDefinition Width="0.30*"/>
                        </Grid.ColumnDefinitions>
                        <Button Grid.Column="0" Command="{Binding AddDoseCountsCommand}" Background="Black" Foreground="White" Content="+" Height="50" FontSize="15"/>
                        <Button Grid.Column="1" FontWeight="Bold" FontSize="15" Height="50" HorizontalAlignment="Stretch" Foreground="White" BorderBrush="#232323" Background="Black" Command="{Binding SelectDoseCountsCommand}" Content="{Binding DoseCounts}" Margin="5" />
                        <Button Grid.Column="2" Command="{Binding SuddDoseCountsCommand}" Background="Black" Foreground="White" Content="-" Height="50" FontSize="15"/>

                    </Grid>
                    <Button BorderBrush="#232323" Background="Black" FontWeight="Bold" FontSize="15" Height="50" IsEnabled="{Binding StartPowderApplyIsEnabled}" Command="{Binding StartApplyPowderCommand}" HorizontalAlignment="Stretch" Margin="5">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Cursor="">
                            <Image Source="/images/powder.png" Width="20" Height="20" Margin="0,0,8,0" />
                                <TextBlock Foreground="White" Text="Нанести порошок" />
                            </StackPanel>
                        </Button>
                        <TextBlock Foreground="Gray" FontWeight="Bold" Margin="5 15 0 0">Положение рекоутера:</TextBlock>
                        <TextBlock Foreground="White" FontWeight="Bold" Margin="5 0 0 0" Text="{Binding RecouterPositionString}"/>

                    </componentsViews:Container>
            
                    <componentsViews:Container Title="Детали" TitleSize="15" Margin="0 20 0 0" Padding="15" Height="262">
                        <ListBox x:Name="mainViewGrid" ItemsSource="{Binding Parts}"
             Height="200"
             Margin="0 0 0 0"
             SelectedItem="{Binding SelectedItem, Mode=TwoWay}"
             BorderThickness="0"
             Background="Transparent"
             VerticalAlignment="Top"
             ScrollViewer.HorizontalScrollBarVisibility="Disabled"
             ScrollViewer.VerticalScrollBarVisibility="Auto" Cursor="">

                            <!-- Стиль контейнера элемента (ListBoxItem) -->
                            <ListBox.ItemContainerStyle>
                                <Style TargetType="ListBoxItem">
                                    <Setter Property="Focusable" Value="False"/>
                                    <Setter Property="IsTabStop" Value="False"/>
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="ListBoxItem">
                                                <Border x:Name="itemBorder"
                                                       Margin="0 0 0 5"
                                                       Padding="10"
                                                       BorderThickness="2 0 0 0"
                                                       BorderBrush="#ff6f00"
                                                       CornerRadius="0"
                                                       Background="#191919"
                                                       Cursor="Hand">
                                                    <StackPanel IsHitTestVisible="False">
                                                        <TextBlock VerticalAlignment="Center" TextWrapping="Wrap" x:Name="itemText" Margin="0 0 0 0" FontWeight="Regular" FontSize="12" Foreground="White" HorizontalAlignment="Center" Text="{Binding Name}"/>
                                                    </StackPanel>


                                                    <b:Interaction.Triggers>
                                                        <b:EventTrigger EventName="PreviewMouseLeftButtonDown">
                                                            <b:InvokeCommandAction
                                       Command="{Binding DataContext.SelectOrderCommand, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                       CommandParameter="{Binding}" />
                                                        </b:EventTrigger>
                                                    </b:Interaction.Triggers>
                                                </Border>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </ListBox.ItemContainerStyle>
                            <ListBox.Template>
                                <ControlTemplate TargetType="ListBox">
                                    <ScrollViewer x:Name="ScrollViewer"
                                                  Padding="{TemplateBinding Padding}"
                                                  Focusable="false">
                                        <ItemsPresenter />
                                    </ScrollViewer>
                                </ControlTemplate>
                            </ListBox.Template>


                    </ListBox>
                        
                </componentsViews:Container>
                </StackPanel>
            </Border>

        <Canvas>
            <Border  Margin="20" Padding="15" Background="Black" CornerRadius="15">
                <StackPanel Orientation="Horizontal">
                    <TextBlock VerticalAlignment="Center" Foreground="White" FontWeight="Black" FontSize="20">Печать проекта</TextBlock>

                    <Border MouseDown="OnCameraSelect" BorderBrush="Black" BorderThickness="1" Margin="200 0 0 0" Background="#1b1b1b" Padding="5" Width="200px" CornerRadius="10">
                        <WrapPanel>
                            <Image Source="/images/camera.png" Width="40"/>
                            <TextBlock Margin="10 0 0 0" Foreground="White" FontWeight="Bold" FontSize="15" VerticalAlignment="Center">Камера</TextBlock>
                        </WrapPanel>
                    </Border>
                    <Border MouseDown="On2DSelect" BorderBrush="Black" BorderThickness="1" Margin="10 0 0 0" Background="#1b1b1b" Padding="5" Width="200px" CornerRadius="10">
                        <WrapPanel>
                            <Image Source="/images/2d.png" Width="40"/>
                            <TextBlock Margin="10 0 0 0" Foreground="White" FontWeight="Bold" FontSize="15" VerticalAlignment="Center">2D превью</TextBlock>
                        </WrapPanel>
                    </Border>

                    <ToggleButton IsChecked="{Binding IsOnlineMode, Mode=TwoWay}"
                                  BorderBrush="Black"
                                  BorderThickness="1"
                                  Margin="10 0 0 0"
                                  Padding="15 10"
                                  Cursor="Hand">
                        <ToggleButton.Style>
                            <Style TargetType="ToggleButton">
                                <Setter Property="Background" Value="#1b1b1b"/>
                                <Setter Property="Foreground" Value="White"/>
                                <Setter Property="FontWeight" Value="Bold"/>
                                <Setter Property="FontSize" Value="15"/>
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="ToggleButton">
                                            <Border Background="{TemplateBinding Background}"
                                                    BorderBrush="{TemplateBinding BorderBrush}"
                                                    BorderThickness="{TemplateBinding BorderThickness}"
                                                    CornerRadius="10"
                                                    Padding="{TemplateBinding Padding}">
                                                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            </Border>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                                <Style.Triggers>
                                    <Trigger Property="IsChecked" Value="True">
                                        <Setter Property="Background" Value="#ff6f00"/>
                                    </Trigger>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="Opacity" Value="0.8"/>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </ToggleButton.Style>
                        <TextBlock Text="РЕЖИМ ОНЛАЙН" Foreground="White" FontWeight="Bold" FontSize="15"/>
                    </ToggleButton>
                </StackPanel>
            </Border>

            <!-- ТЕСТОВЫЕ СКРОЛЛБАРЫ ДЛЯ ЭМУЛЯЦИИ ПРОГРЕССА СКАНАТОРОВ --><!--
            <Border Margin="20,100,20,20" Padding="15" Background="Black" CornerRadius="15" HorizontalAlignment="Left" VerticalAlignment="Top">
                <StackPanel>
                    <TextBlock Foreground="White" FontWeight="Bold" FontSize="14" Margin="0,0,0,10">ТЕСТ: Прогресс сканаторов</TextBlock>

                    --><!-- Сканатор 1 (227, LaserNum=1) --><!--
                    <StackPanel Orientation="Horizontal" Margin="0,0,0,10">
                        <TextBlock Foreground="Cyan" FontWeight="Bold" FontSize="12" Width="120" VerticalAlignment="Center">Сканатор 1 (227):</TextBlock>
                        <Slider x:Name="Scanner1Slider"
                                Minimum="0"
                                Maximum="100"
                                Value="{Binding TestScanner1Progress, Mode=TwoWay}"
                                Width="300"
                                VerticalAlignment="Center"
                                Margin="10,0,10,0"/>
                        <TextBlock Foreground="White" FontSize="12" Width="50" VerticalAlignment="Center">
                            <Run Text="{Binding TestScanner1Progress, StringFormat={}{0:F0}}"/>
                            <Run Text="%"/>
                        </TextBlock>
                    </StackPanel>

                    --><!-- Сканатор 2 (228, LaserNum=0) --><!--
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Foreground="Orange" FontWeight="Bold" FontSize="12" Width="120" VerticalAlignment="Center">Сканатор 2 (228):</TextBlock>
                        <Slider x:Name="Scanner2Slider"
                                Minimum="0"
                                Maximum="100"
                                Value="{Binding TestScanner2Progress, Mode=TwoWay}"
                                Width="300"
                                VerticalAlignment="Center"
                                Margin="10,0,10,0"/>
                        <TextBlock Foreground="White" FontSize="12" Width="50" VerticalAlignment="Center">
                            <Run Text="{Binding TestScanner2Progress, StringFormat={}{0:F0}}"/>
                            <Run Text="%"/>
                        </TextBlock>
                    </StackPanel>
                </StackPanel>
            </Border>-->
        </Canvas>
    </Grid>
</UserControl>
